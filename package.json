{
  "name": "minium",
  "version": "0.1.0",
  "description": "Cross-platform Electron-based budgeting tool",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "build:main": "NODE_ENV=development webpack --config webpack.config.main.babel.js",
    "build:preload": "NODE_ENV=development webpack --config webpack.config.preload.babel.js",
    "clean:build": "rimraf build",
    "coverage": "codecov",
    "electron": "electron build",
    "lint": "eslint \"src/**/*.@(j|t)s?(x)\"",
    "postinstall": "npm rebuild zeromq --runtime=electron && electron-rebuild",
    "prebuild": "npm run clean:build",
    "prebuild:main": "npm run clean:build",
    "preelectron": "run-p -n build:*",
    "start": "run-p -nr watch:* electron",
    "test": "jest",
    "typings": "tsc",
    "watch:renderer": "NODE_ENV=development webpack-dev-server --config webpack.config.renderer.babel.js",
    "watch:server": "NODE_ENV=development webpack-dev-server --config webpack.config.server.babel.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dilatorily/minium.git"
  },
  "keywords": [
    "electron",
    "budgeting",
    "react",
    "redux",
    "windows",
    "macos",
    "linux"
  ],
  "author": "Huy Dang Lê-Ngô",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Dilatorily/minium/issues"
  },
  "homepage": "https://github.com/Dilatorily/minium#readme",
  "dependencies": {
    "core-js": "3.4.0",
    "electron": "6.0.12",
    "get-port": "5.0.0",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "regenerator-runtime": "0.13.3",
    "zeromq": "5.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.7.0",
    "@babel/plugin-proposal-class-properties": "7.7.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.4.4",
    "@babel/plugin-proposal-optional-chaining": "7.6.0",
    "@babel/preset-env": "7.7.1",
    "@babel/preset-react": "7.7.0",
    "@babel/preset-typescript": "7.7.0",
    "@babel/register": "7.7.0",
    "@commitlint/config-conventional": "8.2.0",
    "@hot-loader/react-dom": "16.10.2",
    "@testing-library/jest-dom": "4.2.3",
    "@testing-library/react": "9.3.2",
    "@types/electron-devtools-installer": "2.2.0",
    "@types/jest": "24.0.22",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.3",
    "@types/zeromq": "4.6.2",
    "@typescript-eslint/eslint-plugin": "2.6.1",
    "@typescript-eslint/parser": "2.6.1",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "codecov": "3.6.1",
    "commitlint": "8.2.0",
    "electron-debug": "3.0.1",
    "electron-devtools-installer": "2.2.4",
    "electron-rebuild": "1.8.6",
    "eslint": "6.6.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.5.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "23.0.5",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.2.0",
    "eslint-plugin-testing-library": "1.3.2",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.0.9",
    "jest": "24.9.0",
    "lint-staged": "9.4.2",
    "node-loader": "0.6.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "react-hot-loader": "4.12.15",
    "rimraf": "3.0.0",
    "typescript": "3.7.2",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.9.0"
  }
}
